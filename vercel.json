{
  "buildCommand": "npx vite build --config vite.config.prod.ts",
  "outputDirectory": "dist/public",
  "installCommand": "npm install",
  "devCommand": "npm run dev:client",
  "framework": null,
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
